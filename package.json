{
  "name": "vite-plugins",
  "version": "0.0.1",
  "description": "vite.js plugins",
  "keywords": [
    "vite",
    "plugin"
  ],
  "packageManager": "pnpm@7.9.4",
  "scripts": {
    "preinstall": "node scripts/preinstall",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:pages": "pnpm --filter vite-plugin-page-html run build"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.js"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write"
    ],
    "*.ts": [
      "prettier --parser=typescript --write"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meqn/vite-plugins.git"
  },
  "author": "Mervin <mengqing723@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Meqn/vite-plugins/issues"
  },
  "homepage": "https://github.com/Meqn/vite-plugins#readme",
  "devDependencies": {
    "@types/ejs": "^3.1.1",
    "@types/fs-extra": "^9.0.13",
    "@types/lodash": "^4.14.185",
    "@types/node": "^18.7.18",
    "chalk": "4.1.2",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.3",
    "typescript": "^4.8.3",
    "vite": "^3.1.3",
    "yorkie": "^2.0.0"
  }
}
