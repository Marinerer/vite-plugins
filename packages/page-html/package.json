{
  "name": "vite-plugin-page-html",
  "version": "2.0.2",
  "description": "More flexible MPA (Multi-Page App) support for vite. 简单灵活的 MPA(多页应用) Vite插件。",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "tsup-node src/index.ts --watch --ignore-watch examples",
    "build": "rimraf -rf dist && tsup-node src/index.ts --format esm,cjs --dts"
  },
  "keywords": [
    "vite",
    "mpa",
    "multi-page",
    "vite-plugin",
    "vite-ejs",
    "vite-mpa",
    "vite-html",
    "vite-pages",
    "vite-multi-page",
    "vite-external",
    "vite-cdn"
  ],
  "engines": {
    "node": ">=14.0.0"
  },
  "homepage": "https://github.com/Meqn/vite-plugins",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Meqn/vite-plugins.git"
  },
  "author": "Mervin <mengqing723@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Meqn/vite-plugins/issues"
  },
  "dependencies": {
    "@rollup/pluginutils": "^4.2.1",
    "connect-history-api-fallback": "^2.0.0",
    "diy-log": "^2.4.0",
    "ejs": "^3.1.8",
    "fs-extra": "^10.1.0",
    "html-minifier-terser": "^7.0.0",
    "lodash-es": "^4.17.21",
    "pathe": "^0.3.9"
  },
  "peerDependencies": {
    "vite": ">=2.0.0"
  }
}
